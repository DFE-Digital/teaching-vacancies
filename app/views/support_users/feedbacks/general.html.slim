- content_for :page_title_prefix, t(".title")

- content_for :breadcrumbs do
  nav.govuk-breadcrumbs aria-label="Breadcrumbs"
  = govuk_back_link text: t("buttons.back"), href: support_user_root_path

.govuk-grid-row
  .govuk-grid-column-full = render "tabs"

.govuk-grid-row
  .govuk-grid-column-full = render "reporting_period_control"

.govuk-grid-row
  .govuk-grid-column-full = render "tables"
- download_params = { reporting_period: { from: @reporting_period.from, to: @reporting_period.to }, format: :csv }
= link_to "Download general feedback report", support_users_feedback_general_path(download_params)

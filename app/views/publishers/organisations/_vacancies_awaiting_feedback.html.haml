- if vacancies.any?
  %p.govuk-body#awaiting_feedback_intro=t("jobs.awaiting_feedback_intro")

  %ul.govuk-list#awaiting_feedback{ class: "govuk-!-margin-top-4" }
    %li.job-title-table-header= table_header_sort_by(t("jobs.job_title"), column: "job_title", sort: sort)
    - if organisation.is_a?(SchoolGroup)
      %li= table_header_sort_by(t("jobs.location"), column: "readable_job_location", sort: sort)
    %li= table_header_sort_by(t("jobs.expired_on"), column: "expires_on", sort: sort)

  .expired__vacancy-form
    - vacancies.each do |vacancy|
      = render "publishers/organisations/vacancy_awaiting_feedback", vacancy: vacancy, organisation: organisation
- else
  = render(Shared::NotificationComponent.new(content: { body: t("jobs.no_awaiting_feedback") }, style: 'empty', links: nil, dismiss: false, background: true, alert: false))

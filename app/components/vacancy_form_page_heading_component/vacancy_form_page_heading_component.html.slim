- content_for :breadcrumbs do
  = govuk_back_link text: t("buttons.back"), href: back_path

- if @fieldset
  span.govuk-caption-l
    = caption
  = heading

- else
  h1 class=heading_class
    span.govuk-caption-l
      = caption
    = heading
    span.govuk-caption-m class=sub_caption_class
      = sub_caption
